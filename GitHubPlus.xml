  <style name="php:string" foreground="#dd1144"/>
  <style name="python:builtin-constant" foreground="#000000" bold="true"/>
  <style name="python:builtin-function" foreground="#990000" bold="true"/>
  <style name="python:module-handler" foreground="#000000" bold="true"/>
  <style name="python:special-variable" foreground="#000000" bold="true"/>
