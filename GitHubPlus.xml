<?xml version="1.0" encoding="UTF-8"?>

<style-scheme id="githubplus" _name="GitHubPlus" version="1.0">
  <author>tm2gtksw2 and Tad Vizbaras</author>
  <_description>GitHubPlus Theme - Generated and hand edited from textmate theme</_description>
  
  <style name="search-match" background="#f0f090" underline="true"/>
  <style name="selection" foreground="#f0f0f0" background="#DD4814"/>  <!-- foreground="#f0f0f5" background="#428bca" -->
  
  <style name="text" foreground="#000000" background="#f0f0f8"/>

  <style name="bracket-match"  background="#e2e2e9" bold="true"/>
  <style name="bracket-mismatch"  background="#e2e2e9" underline="true" bold="true"/>
  
  <style name="css:at-rules" foreground="#000000" bold="true"/>
  <style name="css:keyword"  bold="true"/>
  
  <style name="current-line"  background="#e6e6f0"/>
  <style name="cursor" foreground="#000000"/>
  <style name="def:base-n-integer" foreground="#009999"/>
  <style name="def:boolean" foreground="#000000" bold="true"/>
  <style name="def:builtin" foreground="#000000" bold="true"/>
  <style name="def:character" foreground="#dd1144"/>
  <style name="def:comment" foreground="#999988" italic="true"/>
  <style name="def:complex" foreground="#009999"/>
  <style name="def:decimal" foreground="#009999"/>
  <style name="def:doc-comment" foreground="#999988" italic="true"/>
  <style name="def:doc-comment-element" foreground="#999988" italic="true"/>
  <style name="def:floating-point" foreground="#009999"/>
  <style name="def:function" foreground="#990000" bold="true"/>
  <style name="def:identifier" foreground="#108888"/>
  <style name="def:keyword" foreground="#000000" bold="true"/>
  <style name="def:note" foreground="#999988" italic="true" bold="true"/>
  <style name="def:number" foreground="#009999"/>
  <style name="def:operator" foreground="#000000" bold="true"/>
  <style name="def:preprocessor" foreground="#000000" bold="true"/>
  <style name="def:reserved" foreground="#000000" bold="true"/>
  <style name="def:shebang" foreground="#999988" italic="true"/>
  <style name="def:special-constant" foreground="#000000" bold="true"/>
  
  <style name="def:statement" foreground="#000000" bold="true"/>
  <style name="def:type" foreground="#106096" bold="true"/>
  
  <style name="def:string" foreground="#dd1144"/>
  
  <style name="draw-spaces" foreground="#bfbfbf"/>
  <style name="html:dtd" foreground="#dd1144"/>
  <style name="html:tag" foreground="#121289"/>
  <style name="line-numbers" foreground="#909090" background="#e6e6ed"/>
  <style name="perl:builtin" foreground="#990000" bold="true"/>
  <style name="perl:include-statement" foreground="#000000" bold="true"/>
  <style name="perl:special-variable" foreground="#000000" bold="true"/>
