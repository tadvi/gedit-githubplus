  <style name="ruby:builtin" foreground="#108888"/>
  <style name="ruby:class-variable" foreground="#108888"/>
  <style name="ruby:constant" foreground="#108888"/>
  <style name="ruby:global-variable" foreground="#990000" bold="true"/>
  <style name="ruby:instance-variable" foreground="#108888"/>
  <style name="ruby:module-handler" foreground="#000000" bold="true"/>
  <style name="ruby:predefined-variable" foreground="#000000" bold="true"/>
  <style name="ruby:regex" foreground="#009926"/>
  <style name="ruby:special-variable" foreground="#000000" bold="true"/>
  <style name="ruby:symbol" foreground="#aa2c8c"/>
